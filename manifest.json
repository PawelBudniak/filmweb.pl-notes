{
    "manifest_version": 2,
    "name": "Filmweb notes",
    "version": "1.0",
    "description": "Filmweb notes",
    "permissions": [
      "storage",
      "activeTab",
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*.filmweb.pl/film/*"
        ],
        "js": [
          "content.js"
        ],
        "run_at": "document_end"
      },
      {
        "matches": [
          "*://*.filmweb.pl/user/*/wantToSee"
        ],
        "js": [
          "want-to-see.js"
        ],
        "run_at": "document_end"
      }
    ]
}
